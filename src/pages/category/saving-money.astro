    {
      "@type": "CollectionPage",
      "@id": "https://buzznum.com/category/saving-money/",
      "@url": "https://buzznum.com/category/saving-money/",
      "@name":
        "Saving Money Archives - Buzznum | Secure second and temporary phone number app",
