  title: "Family & Friends Archives - Hushed | Secure second and temporary phone number app",
  description: "Welcome to the Hushed blog, where we share tips on digital privacy, tech, and how to use your Hushed phone numbers.",
  ogLocale: "en_US", 
  ogType: "article",
  ogTitle: "Family & Friends Archives - Hushed | Secure second and temporary phone number app",
  ogUrl: "https://hushed.com/category/family/",
  ogSiteName: "Hushed | Secure second and temporary phone number app",
  twitterCard: "summary_large_image",
  canonicalUrl: "https://hushed.com/category/family/",
  nextPageUrl: "https://hushed.com/category/family/page/2/",
  preloadImage: {
    srcset: "/images/family-friends-100x0-c-default.webp 100w, /images/family-friends-200x0-c-default.webp 200w, /images/family-friends-400x0-c-default.webp 400w, /images/family-friends-600x0-c-default.webp 600w, /images/family-friends-800x0-c-default.webp 800w, /images/family-friends-1000x0-c-default.webp 1000w, /images/family-friends.webp 1200w",
    sizes: "(max-width: 767px) 300px, 755px"
  }
};

const jsonLd = {
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "CollectionPage",
      "@id": "https://hushed.com/category/family/",
      "url": "https://hushed.com/category/family/",
      "name": "Family & Friends Archives - Hushed | Secure second and temporary phone number app",
      "isPartOf": { "@id": "https://hushed.com/#website" },
      "primaryImageOfPage": { "@id": "https://hushed.com/category/family/#primaryimage" },
      "image": { "@id": "https://hushed.com/category/family/#primaryimage" },
      "thumbnailUrl": "https://hushed.com/wp-content/uploads/2024/12/family-friends.png",
      "breadcrumb": { "@id": "https://hushed.com/category/family/#breadcrumb" },
      "inLanguage": "en-US"
    }
