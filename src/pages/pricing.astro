    <link
    rel="stylesheet"
    id="block-faq-css"
    href={config.urls.origin + '/css/blocks/faq.css'}
    type="text/css"
    media="all"
  />
    <link
      rel="stylesheet"
      id="app-css"
      href={config.urls.origin + '/css/app.css'}
      type="text/css"
      media="all"
    />
    <link
      rel="stylesheet"
      id="template-index-css"
      href={config.urls.origin + '/css/templates/index.css'}
      type="text/css"
